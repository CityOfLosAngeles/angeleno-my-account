{
  "hosting": {
    "target": "angeleno-my-account-sandbox-422814",
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/maps/**",
        "function": "maps",
        "region": "us-west1"
      },
      {
        "source": "/auth0/**",
        "function": "auth0",
        "region": "us-west1"
      }
    ]
  },
  "functions": [
    {
      "source": "functions/auth0",
      "codebase": "auth0",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
    },
    {
      "source": "functions/maps",
      "codebase": "maps",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
    }
  ]
}
