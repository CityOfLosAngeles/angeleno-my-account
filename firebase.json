{
  "hosting": {
    "target": "angeleno-my-account-dev-402917",
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/updateUser",
        "function": "updateUser",
        "region": "us-central1",
        "pinTag": true
      },
      {
        "source": "/updatePassword",
        "function": "updatePassword",
        "region": "us-central1"
      }
    ]
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
    }
  ]
}
